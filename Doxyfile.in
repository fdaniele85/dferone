PROJECT_NAME           = "dferone"
PROJECT_NUMBER         = @PROJECT_VERSION@
PROJECT_BRIEF          = "C++20 header-only library for optimization algorithms"

OUTPUT_DIRECTORY       = .

INPUT                  = @CMAKE_CURRENT_SOURCE_DIR@/include @CMAKE_CURRENT_SOURCE_DIR@/README.md
USE_MDFILE_AS_MAINPAGE = @CMAKE_CURRENT_SOURCE_DIR@/README.md

EXTRACT_ALL            = YES
HIDE_UNDOC_MEMBERS     = NO
HIDE_UNDOC_CLASSES     = NO

EXCLUDE                = @CMAKE_CURRENT_SOURCE_DIR@/build* @CMAKE_CURRENT_SOURCE_DIR@/cmake-build-* @CMAKE_CURRENT_SOURCE_DIR@/_deps @CMAKE_CURRENT_SOURCE_DIR@/tests @CMAKE_CURRENT_SOURCE_DIR@/doc @CMAKE_CURRENT_SOURCE_DIR@/.git @CMAKE_CURRENT_SOURCE_DIR@/.github
USE_MDFILE_AS_MAINPAGE = README.md

EXTRACT_ALL            = YES
EXTRACT_PRIVATE        = NO
EXTRACT_STATIC         = YES

HIDE_UNDOC_MEMBERS     = NO
HIDE_UNDOC_CLASSES     = NO

GENERATE_HTML          = YES
HTML_OUTPUT            = html
HTML_FILE_EXTENSION    = .html

GENERATE_LATEX         = NO
GENERATE_RTF           = NO
GENERATE_MAN           = NO
GENERATE_XML           = NO

HAVE_DOT               = NO
CLASS_DIAGRAMS         = YES

SEARCHENGINE           = YES
FILE_PATTERNS = *.h
RECURSIVE = YES
